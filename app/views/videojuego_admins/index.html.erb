<div class="col-sm-9">
<h1>Games FCA</h1>
<hr>
</div> 

<div class="col-sm-9"> 
<div class="panel panel-default">
<div class="panel-heading">Modificaciones de los administradores</div>
<table class="table">
  <thead>
    <tr>
      <th>Videojuego</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @videojuego_admins.each do |videojuego_admin| %>
      <tr>
        <td><%= videojuego_admin.videojuego_id %></td>
        <td><%= videojuego_admin.admin_id %></td>
        <td><%= link_to 'Show', videojuego_admin, :class=>"btn btn-default" %></td>
        <td><%= link_to 'Edit', edit_videojuego_admin_path(videojuego_admin), :class=>"btn btn-default" %></td>
        <td><%= link_to 'Destroy', videojuego_admin, method: :delete, data: { confirm: 'Are you sure?' },:class=>"btn btn-default" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
 </div>
</div>
<br>
<div class="col-sm-9">
<%= link_to 'Agregar Registro', new_videojuego_admin_path, :class=>"btn btn-default" %>
</div>